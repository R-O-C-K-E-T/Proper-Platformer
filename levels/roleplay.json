  "serverScript": "\ndef hideObstacle(self, other, normal, localA, localB):\n   if self in waiting:\n      waiting[self] = time, waiting[self][1]\n      return\n\n   self.colour = 200, 0, 0\n   obstacle = next(filter(lambda obj: self.groups[0] in obj.groups, obstacles))\n   \n   def func():\n      add_object(obstacle)\n   \n   removeObject(obstacle)\n   waiting[self] = time, func\n\ndef showPlatform(self, other, normal, localA, localB):\n    if self in waiting:\n        waiting[self] = time, waiting[self][1]\n        return\n    \n    self.colour = 200, 0, 0\n    platform = next(filter(lambda obj: self.groups[0] in obj.groups, hiddenPlatforms))\n    \n    def func():\n        removeObject(platform)\n    \n    add_object(platform)\n    waiting[self] = time, func\n\ndef hideBeam(self, other, normal, localA, localB):\n    global beamButton\n    if beamButton is not None:\n        beamButton = time, beamButton[1]\n        return\n    \n    removeObject(beam)\n    \n    self.colour = 200, 0, 0\n    def func():\n        self.colour = self.initialState['colour']\n        add_object(beam)\n    beamButton = time, func\n\ndef movePlatformLeft(self, other, normal, localA, localB):\n   group = self.groups[0]\n   if group == '0':\n       dist = 554\n   elif group == '1':\n       dist = 300\n   elif group == '2':\n       dist = 312\n\n   for platform in platforms:\n      if group not in platform.groups:\n         continue\n      if platform.pos[0]+2 > dist + platform.initialState['pos'][0]:\n         target = dist + platform.initialState['pos'][0]\n         platform.vel = target - platform.pos[0], 0\n      else:\n         platform.vel = 2,0\n   self.colour = 200, 0, 0\n\n   def func():\n      for platform in platforms:\n         platform.vel = 0,0\n   \n   waiting[self] = time, func\n\ndef movePlatformRight(self, other, normal, localA, localB):\n   group = self.groups[0]\n   for platform in platforms:\n      if group not in platform.groups:\n         continue\n      if platform.pos[0]-2 < platform.initialState['pos'][0]:\n         platform.vel = platform.initialState['pos'][0]-platform.pos[0], 0\n      else:\n         platform.vel = -2,0\n   self.colour = 200, 0, 0\n\n   def func():\n      for platform in platforms:\n         platform.vel = 0,0\n   \n   waiting[self] = time, func\n\ndef onDeath(player): # Called whenever any player dies.\n    pass\n\ndef load(): # Will be called every time physics is reset\n    global platforms, waiting, obstacles, hiddenPlatforms, beam, beamButton, lilypads\n    platforms = getGroup('Platform')\n    obstacles = getGroup('Obstacle')\n    hiddenPlatforms = getGroup('HiddenPlatform')\n    for obj in hiddenPlatforms:\n        removeObject(obj)\n    beam = getGroup('Beam')[0]\n    beamButton = None\n    lilypads = []\n    waiting = {}\n\ndef tick(): # Will be called once per tick\n    global beamButton\n    for obj, (t, callback) in waiting.copy().items():\n       if t + 3 < time:\n          obj.colour = obj.initialState['colour']\n          callback()\n          del waiting[obj]\n    \n    if beamButton is not None and beamButton[0] + 20 < time:\n        beamButton[1]()\n        beamButton = None\n\n    if beamButton is None and all(obj.vel[0] < 2.5 for obj in lilypads):\n        obj = createObject({'physics':{'density':0.3}, 'colour':[217,217,0], 'type': 'polygon', 'points': [[-30,-10],[30,-10],[30,10],[-30,10]]})\n        obj.pos = 3710, -350\n        obj.vel = 3, 0\n        lilypads.append(obj)\n        if len(lilypads) > 3:\n            removeObject(lilypads.pop(0))\n    for obj in lilypads.copy():\n        if obj.pos[0] > 4930:\n            lilypads.remove(obj)\n            removeObject(obj)\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n",
  "clientScript": "def hideObstacle(self, other, normal, localA, localB):\n   if self in waiting:\n      waiting[self] = time, waiting[self][1]\n      return\n\n   self.colour = 200, 0, 0\n   def func():\n      pass\n   waiting[self] = time, func\n\ndef showPlatform(self, other, normal, localA, localB):\n    if self in waiting:\n        waiting[self] = time, waiting[self][1]\n        return\n    \n    self.colour = 200, 0, 0\n    def func():\n        pass\n    waiting[self] = time, func\n\ndef hideBeam(self, other, normal, localA, localB):\n    global beamButton\n    if beamButton is not None:\n        beamButton = time, beamButton[1]\n        return\n    \n    self.colour = 200, 0, 0\n    def func():\n        self.colour = self.initialState['colour']\n    beamButton = time, func\n\ndef movePlatformLeft(self, other, normal, localA, localB):\n   group = self.groups[0]\n   if group == '0':\n       dist = 554\n   elif group == '1':\n       dist = 300\n   elif group == '2':\n       dist = 312\n\n   for platform in platforms:\n      if group not in platform.groups:\n         continue\n      if platform.pos[0]+2 > dist + platform.initialState['pos'][0]:\n         target = dist + platform.initialState['pos'][0]\n         platform.vel = target - platform.pos[0], 0\n      else:\n         platform.vel = 2,0\n   self.colour = 200, 0, 0\n\n   def func():\n      for platform in platforms:\n         platform.vel = 0,0\n   \n   waiting[self] = time, func\n\ndef movePlatformRight(self, other, normal, localA, localB):\n   group = self.groups[0]\n   for platform in platforms:\n      if group not in platform.groups:\n         continue\n      if platform.pos[0]-2 < platform.initialState['pos'][0]:\n         platform.vel = platform.initialState['pos'][0]-platform.pos[0], 0\n      else:\n         platform.vel = -2,0\n   self.colour = 200, 0, 0\n\n   def func():\n      for platform in platforms:\n         platform.vel = 0,0\n   \n   waiting[self] = time, func\n\ndef onDeath(player): # Called whenever any player dies.\n    pass\n\ndef load(): # Will be called every time physics is reset\n    global platforms, waiting, obstacles, beamButton\n    platforms = []#getGroup('Platform')\n    obstacles = []#getGroup('Obstacle')\n    beamButton = None\n    waiting = {}\n\ndef add_object(obj):\n    if not isinstance(obj, Object):\n        return\n    if 'Platform' in obj.groups:\n        platforms.append(obj)\n    if 'Obstacle' in obj.groups:\n        obstacles.append(obj)\n\ndef tick(): # Will be called once per tick\n    global beamButton\n    for obj, (t, callback) in waiting.copy().items():\n       if t + 3 < time:\n          obj.colour = obj.initialState['colour']\n          callback()\n          del waiting[obj]\n\n    if beamButton is not None and beamButton[0] + 20 < time:\n        beamButton[1]()\n        beamButton = None\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n",
